<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        table, td, th {
          border: 1px solid black;
        }
        table {
          width: 30%;
          border-collapse: collapse;
        }
        </style>
</head>
<body>
    <h1>메모장1</h1>
    <p><a href="/create1">메모 추가</a></p>
    <input type="button" value="메모 추가" onclick="location.href='/create1'"> <br>
    <table>
        <tr>
            <th>title</th>
            <th>memo</th>
            <th>수정</th>
            <th>삭제</th>
        </tr>
        <% memo1.forEach(function (value) { %>
        <tr>
            <td><%=value.title %></td>
            <td><%=value.memo %></td>
            <td><a href="/edit1/<%= value.title %>">수정</a></td>
            <td><a href="/delete1/<%= value.title %>">삭제</a></td>
        </tr>
        <% });  %>
    </table>
</body>
</html>